generator client {
  provider = "prisma-client-js"
}
// Migrate: tạo và cập nhật các bảng trong database
datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id        Int      @id @default(autoincrement())
  email     String   @unique
  password  String
  createdAt DateTime @default(now())
}


